    "version": "0.2.0",
    "configurations": [
        {
            "name": "record",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/src/wpr.go",
            "cwd": "${workspaceRoot}",
            "showLog": false,
            "args": [
                "record",
                "--http_port=8080",
                "--https_port=8081",
                "/tmp/webdata/golang.org.wprgo"
            ]
        },
        {
            "name": "replay",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/src/wpr.go",
            "cwd": "${workspaceRoot}",
            "showLog": false,
            "args": [
                "replay",
                "--http_port=8080",
                "--https_port=8081",
                "/tmp/webdata/HttpArchive.wprgo"
            ]
        },
        {
            "name": "convert",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceRoot}/src/wpr.go",
            "cwd": "${workspaceRoot}",
            "showLog": false,
            "args": [
                "convert",
                "--har_file=/home/yongnian/Downloads/Http Archive/chrome-Apr_1_2016",
                "--https_cert_file=wpr_cert.pem",
                "--https_key_file=wpr_key.pem"
            ]
        }
    ]
}